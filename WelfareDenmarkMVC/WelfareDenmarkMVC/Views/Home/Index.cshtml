@{
    ViewData["Title"] = "Hjem";
}

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="https://images.unsplash.com/photo-1526635090919-b5d79657c5a3?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=c039ad0b8389b47ec6034b2a0538b6d4&auto=format&fit=crop&w=1050&q=80" alt="ASP.NET" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                    Alle elsker at kunne huske sin nevøs navn
                    <a class="btn btn-default" href="/Account/Register">
                        Ja fanme ja
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="https://images.unsplash.com/photo-1508963493744-76fce69379c0?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=6208a3203071968105278c7d0c15587b&auto=format&fit=crop&w=1050&q=80" alt="Visual Studio" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                    "Jeg følte mig dum og imbecil, indtil jeg oprettede min profil"
                    <a class="btn btn-default" href="/Account/Register">
                        Opret profil
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="https://images.unsplash.com/photo-1509732057623-c8a63eb59dd7?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=0083679d5efb2fed98ad984cce0bfd6e&auto=format&fit=crop&w=1050&q=80" alt="Package Management" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <p>
                    Hellere ti ryk i banditten end ti banditter i ryggen
                    <a class="btn btn-default" href="/Account/Register">
                        Enig
                    </a>
                </p>
            </div>
        </div>
        
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
